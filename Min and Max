class Solution:
    def getMinMax(self, arr):
        min=arr[0]
        min_index=0
        max=arr[0]
        max_index=0
        for i in range(1,len(arr)):
            if min>arr[i]:
                min=arr[i]
                min_index=i
            if max<arr[i]:
                max=arr[i]
                max_index=i
        return arr[min_index],arr[max_index]
